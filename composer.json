{
    "name": "idvlab/laravel-yookassa",
    "description": "Yookassa for Laravel",
    "type": "project",
    "license": "MIT",
    "version": "1.1.7",
    "autoload": {
        "psr-4": {
            "idvLab\\LaravelYookassa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "idvLab\\LaravelYookassa\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=8.2",
        "illuminate/support": "~9|~10",
        "imjonos/laravel-base-service": "*",
        "guzzlehttp/guzzle": "^7.2",
        "yoomoney/yookassa-sdk-php": "^3.2",
        "ext-json": "*"
    },
    "authors": [
        {
            "name": "Digkill",
            "email": "digkill@mail.ru"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "idvLab\\LaravelYookassa\\YookassaServiceProvider"
            ],
            "aliases": {
                "YooKassa": "idvLab\\LaravelYookassa\\Facades\\YookassaFacade"
            }
        }
    }
}
